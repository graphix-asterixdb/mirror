// Verify that a vertex cannot be defined with a label more than once.
USE TestDataverse;
WITH
    GRAPH TestGraph AS
        VERTEX (:Vertex1)
            PRIMARY KEY (_id)
            AS GenericDataset,
        EDGE (:Vertex1)-[:EDGE_1]->(:Vertex1)
            SOURCE KEY      (_id)
            DESTINATION KEY (_to_id)
            AS (
                FROM
                    GenericDataset GD
                SELECT
                    GD._id    AS _id,
                    GD._to_id AS _to_id
            )
FROM
    GRAPH TestGraph
        (v:Vertex1)-[]-(),
        (v:Vertex1)-[]-()
SELECT
    v;
