-- param max-warnings:string=3

// There are two dangling vertices of different labels, and zero edges.
FROM
    GRAPH Yelp.YelpGraph
        (u:User),
        (r:Review)
SELECT
    u.user_id   AS user_id,
    r.review_id AS review_id
ORDER BY
    u.user_id,
    r.review_id;
