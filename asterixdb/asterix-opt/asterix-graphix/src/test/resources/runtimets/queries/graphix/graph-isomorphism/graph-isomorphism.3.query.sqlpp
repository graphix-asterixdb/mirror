// (u) should never be equal to (v).
WITH
    GRAPH YelpGraph AS
        VERTEX (:User)
            PRIMARY KEY (user_id)
            AS Yelp.Users
FROM
    GRAPH YelpGraph
        (u),
        (v)
SELECT
    u.user_id AS u_user_id,
    v.user_id AS v_user_id
ORDER BY
    u,
    v;