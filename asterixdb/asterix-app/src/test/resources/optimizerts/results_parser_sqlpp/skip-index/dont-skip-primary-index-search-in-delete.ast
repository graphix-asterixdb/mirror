DataverseUse test
TypeDecl AddressType [
  closed RecordType {
    number : integer,
    street : string,
    city : string
  }
]
TypeDecl CustomerType [
  closed RecordType {
    cid : integer,
    name : string,
    age : integer?,
    address : AddressType?,
    interests : UnorderedList <string>
,
    children : OrderedList [          open RecordType {
            name : string,
            age : integer?
          }
]

  }
]
DatasetDecl Customers(CustomerType) partitioned by [[cid]]
