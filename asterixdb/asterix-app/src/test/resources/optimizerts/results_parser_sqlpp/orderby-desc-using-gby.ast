DataverseUse `gby-using-orderby-desc`
TypeDecl AddressType [
  closed RecordType {
    number : integer,
    street : string,
    city : string
  }
]
TypeDecl CustomerType [
  closed RecordType {
    cid : integer,
    name : string,
    age : integer?,
    address : AddressType?,
    lastorder :       open RecordType {
        oid : integer,
        total : float
      }

  }
]
DatasetDecl Customers(CustomerType) partitioned by [[cid]]
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [name]
    :
    Variable [ Name=$name ]
  )
  (
    LiteralExpr [STRING] [age]
    :
    Variable [ Name=$age ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [gby-using-orderby-desc.Customers]
  ]
  AS Variable [ Name=$c ]
]
Groupby
  Variable [ Name=$name ]
  :=
  FieldAccessor [
    Variable [ Name=$c ]
    Field=name
  ]
  Variable [ Name=$age ]
  :=
  FieldAccessor [
    Variable [ Name=$c ]
    Field=age
  ]
  GROUP AS Variable [ Name=#1 ]
  (
    c:=Variable [ Name=$c ]
  )

Orderby
  Variable [ Name=$name ]
  DESC
  Variable [ Name=$age ]
  ASC

