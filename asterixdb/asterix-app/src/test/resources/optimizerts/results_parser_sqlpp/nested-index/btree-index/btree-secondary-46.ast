DataverseUse test
TypeDecl TestTypetmp [
  open RecordType {
    id : integer,
    fname : string,
    lname : string
  }
]
TypeDecl TestType [
  open RecordType {
    nested : TestTypetmp
  }
]
DatasetDecl testdst(TestType) partitioned by [[nested, id]]
Query:
SELECT ELEMENT [
Variable [ Name=$emp ]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.testdst]
  ]
  AS Variable [ Name=$emp ]
]
Where
  OperatorExpr [
    OperatorExpr [
      FieldAccessor [
        FieldAccessor [
          Variable [ Name=$emp ]
          Field=nested
        ]
        Field=fname
      ]
      >=
      LiteralExpr [STRING] [Michael]
    ]
    and
    OperatorExpr [
      FieldAccessor [
        FieldAccessor [
          Variable [ Name=$emp ]
          Field=nested
        ]
        Field=lname
      ]
      <=
      LiteralExpr [STRING] [Xu]
    ]
  ]
