DataverseUse test
Query:
SELECT ELEMENT [
RecordConstructor [
  (
    LiteralExpr [STRING] [customerid]
    :
    FieldAccessor [
      Variable [ Name=$c ]
      Field=name
    ]
  )
  (
    LiteralExpr [STRING] [unorderedlist]
    :
    Variable [ Name=$n ]
  )
]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.Customers]
  ]
  AS Variable [ Name=$c ]
]
Let Variable [ Name=$rec ]
  :=
  FieldAccessor [
    Variable [ Name=$c ]
    Field=lastorder
  ]
Let Variable [ Name=$m ]
  :=
  OrderedListConstructor [
    FieldAccessor [
      Variable [ Name=$c ]
      Field=cid
    ]
    FieldAccessor [
      Variable [ Name=$rec ]
      Field=oid
    ]
  ]
Let Variable [ Name=$n ]
  :=
  UnorderedListConstructor [
    IndexAccessor [
      Variable [ Name=$m ]
      Index:       LiteralExpr [LONG] [0]
    ]
    IndexAccessor [
      Variable [ Name=$m ]
      Index:       LiteralExpr [LONG] [1]
    ]
    IndexAccessor [
      Variable [ Name=$m ]
      Index:       LiteralExpr [LONG] [4]
    ]
  ]
