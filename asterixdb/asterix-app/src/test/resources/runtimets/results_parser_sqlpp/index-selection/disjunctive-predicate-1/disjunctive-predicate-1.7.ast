DataverseUse test
Set rewrite_or_as_join=false
Query:
SELECT ELEMENT [
Variable [ Name=$x ]
]
FROM [  FunctionCall asterix.dataset@1[
    LiteralExpr [STRING] [test.TestSet]
  ]
  AS Variable [ Name=$x ]
]
Where
  OperatorExpr [
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$x ]
        Field=idx
      ]
      =
      LiteralExpr [STRING] [one]
    ]
    or
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$x ]
        Field=idx
      ]
      =
      LiteralExpr [STRING] [two]
    ]
    or
    OperatorExpr [
      FieldAccessor [
        Variable [ Name=$x ]
        Field=idx
      ]
      =
      LiteralExpr [STRING] [two]
    ]
  ]
Orderby
  FieldAccessor [
    Variable [ Name=$x ]
    Field=id
  ]
  ASC

Limit
  LiteralExpr [LONG] [2]
