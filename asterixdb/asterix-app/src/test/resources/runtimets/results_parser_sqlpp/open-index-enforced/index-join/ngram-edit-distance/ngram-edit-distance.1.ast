DataverseUse test
TypeDecl AddressType [
  open RecordType {
    number : bigint,
    street : string,
    city : string
  }
]
TypeDecl CustomerOpenType [
  open RecordType {
    cid : bigint,
    age : bigint?,
    address : AddressType?,
    interests : OrderedList [string]
,
    children : OrderedList [          open RecordType {
            name : string,
            age : bigint?
          }
]

  }
]
TypeDecl CustomerType [
  closed RecordType {
    cid : bigint,
    name : string,
    age : bigint?,
    address : AddressType?,
    interests : OrderedList [string]
,
    children : OrderedList [          open RecordType {
            name : string,
            age : bigint?
          }
]

  }
]
DatasetDecl Customers(CustomerOpenType) partitioned by [[cid]]
DatasetDecl Customerstmp(CustomerType) partitioned by [[cid]]
DatasetDecl Customers2(CustomerType) partitioned by [[cid]]
