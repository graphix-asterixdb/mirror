    `similarity-jaccard-prefix`(7, [3, 5, 6, 7], 6, [3, 4, 5, 6, 8, 9], 3, 0.3f),
    `similarity-jaccard-prefix`(6, [3, 4, 5, 6, 8, 9], 7, [3, 5, 6, 7], 6, 0.3f),
