/**
 * This query is to verify the fix of ASTERIXDB-1205.
 */

let aa = [{"fa":1, "fb":1}, {"fa":2, "fb":1}],
    bb = [{"fa":1, "fb":0}, {"fa":1, "fb":1}, {"fa":3, "fb":1}],
    ccc = (
        select value x from aa x
        union all
        select value y from bb y
    )
select value tt
from ccc tt
order by tt.fa, tt.fb;
