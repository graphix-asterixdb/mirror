//       Query with LET after the FROM clause.
FROM     ComplexExclude.Users U
LET      miscInfo = { "24as": "23412", "address": "2341 Orange Street" }
SELECT   * EXCLUDE U.address, miscInfo.address, U.phones
ORDER BY U.user_id;