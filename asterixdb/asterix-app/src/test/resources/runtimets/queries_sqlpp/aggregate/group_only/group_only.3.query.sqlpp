/*May be issues with AQL test. By limiting input, the results may change based on storage semantics.*/

USE dblp;

SELECT d.authors AS author, d.title AS title
FROM (SELECT VALUE t
               FROM DBLP t
               ORDER BY id LIMIT 5) AS d
GROUP BY d.authors, d.title
ORDER BY d.authors;

