use test;

WITH cte AS (
  select value 1 union all select value 1 union all select value 1 union all
  SELECT VALUE 5 + v * 2 FROM range(0, 22) v
)
FROM cte x
SELECT x, sum(x) over (ORDER BY x groups between 1 preceding and 1 following) as `sum`
ORDER BY x