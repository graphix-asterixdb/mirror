/*
 * Description  : LAG() RESPECT / IGNORE NULLS
 * Expected Res : SUCCESS
 */

from [
  { "x": "a",         "y": 1,  "p": 0 },
  {                   "y": 2,  "p": 0 },
  { "x": "b",         "y": 3,  "p": 0 },
  { "x": null,        "y": 4,  "p": 0 },
  { "x": "c",         "y": 5,  "p": 0 },
  {                   "y": 6,  "p": 0 },

  { "x": "a",         "y": 7,  "p": 1 },
  { "x": null,        "y": 8,  "p": 1 },
  { "x": "b",         "y": 9,  "p": 1 },
  {                   "y": 10, "p": 1 },
  { "x": "c",         "y": 11, "p": 1 },
  { "x": null,        "y": 12, "p": 1 }
] t
select
  lag(x) RESPECT NULLS over (partition by p order by y)
    as lag_1_respect,
  lag(x) IGNORE NULLS over (partition by p order by y)
    as lag_1_ignore,
  x, y, p
order by y