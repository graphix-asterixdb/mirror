USE test;
SET `compiler.parallelism` "0";
SET `compiler.sort.parallel` "false";
EXPLAIN
SELECT sum(object_length(p1)), sum(object_length(p2))
FROM ColumnDataset p1, RowDataset p2
WHERE p2.id = p1.id;