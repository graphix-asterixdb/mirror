SET `import-private-functions` `true`;

from [
{"id": 1, "f": 1},
{"id": 2, "f": true},
{"id": 3, "f": "test"},
{"id": 4, "f": [1,2]},
{"id": 5, "f": {"f1": 1, "f2": [{"n": "str"}]}}
] AS t
select t.f AS val, serialized_size(t.f) AS serialized_size
order by t.id;