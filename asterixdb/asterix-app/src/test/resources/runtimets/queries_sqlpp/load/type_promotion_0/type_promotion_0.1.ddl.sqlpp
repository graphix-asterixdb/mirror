drop  dataverse TestDataverse if exists;
create  dataverse TestDataverse;

use TestDataverse;


create type TestDataverse.TestType as
{
  id : bigint,
  int8 : tinyint,
  int16 : smallint,
  int32 : integer,
  int64 : bigint,
  float : float,
  double : double,
  int8_u : {{tinyint}}?,
  int8_o : [tinyint]?,
  int16_u : {{smallint}}?,
  int16_o : [smallint]?,
  int32_u : {{integer}}?,
  int32_o : [integer]?,
  int64_u : {{bigint}}?,
  int64_o : [bigint]?,
  float_u : {{float}}?,
  float_o : [float]?,
  double_u : {{double}}?,
  double_o : [double]?
};

create  dataset TestSet(TestType) primary key id;

