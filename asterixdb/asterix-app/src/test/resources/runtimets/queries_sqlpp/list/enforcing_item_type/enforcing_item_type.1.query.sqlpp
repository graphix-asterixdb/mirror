/*
 * Description: testing that each item conforms to the list type including function items.
 */

explain select value {"id": t[0], "v": t[1]}
from [
[ 29, CASE(2 < 1) WHEN true THEN null ELSE {"f1": "a", "f2": 3} END]
] t;

