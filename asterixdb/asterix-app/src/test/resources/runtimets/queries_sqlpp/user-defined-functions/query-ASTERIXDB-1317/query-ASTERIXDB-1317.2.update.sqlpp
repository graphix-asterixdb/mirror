use test;

load dataset HRM_Raw using localfs
    (("path"="asterix_nc1://data/csv/55.csv"),
    ("format"="delimited-text"),
    ("header"="true"));

insert into HRM
(
    select element {
        "row_id": t.row_id,
        "sid" : t.sid,
        "date" : parse_date(t.date, "YMD"),
        "time" : parse_time(t.time, "h:m:s"),
        "day" : t.day,
        "bpm" : t.bpm,
        "RR" : [t.RR]
    }
    from HRM_Raw t
)