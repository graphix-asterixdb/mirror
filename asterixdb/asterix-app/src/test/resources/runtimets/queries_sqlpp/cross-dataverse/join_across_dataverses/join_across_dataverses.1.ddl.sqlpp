drop  dataverse test1 if exists;
drop  dataverse test2 if exists;
create  dataverse test1;

create  dataverse test2;

create type test1.AddressType as
{
  number : bigint,
  street : string,
  city : string
};

create type test1.CustomerType as
 closed {
  cid : bigint,
  name : string,
  cashBack : bigint,
  age : bigint?,
  address : AddressType?,
  lastorder : {
      oid : bigint,
      total : float
  }

};

create  dataset test1.Customers(CustomerType) primary key cid;

create type test2.OrderType as
{
  oid : bigint,
  cid : bigint,
  orderstatus : string,
  orderpriority : string,
  clerk : string,
  total : float,
  items : [bigint]
};

create  dataset test2.Orders(OrderType) primary key oid;

