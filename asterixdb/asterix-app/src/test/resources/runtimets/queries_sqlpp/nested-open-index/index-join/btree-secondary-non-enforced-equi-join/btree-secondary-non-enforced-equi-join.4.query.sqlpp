use test;

select element {'aid':a.nested.id,'bid':b.nested.id,'authors':a.nested.authors}
from  DBLP as a, CSX as b
where to_string(a.nested.authors) /*+ indexnl */  = b.nested.authors
order by a.nested.id, b.nested.id;